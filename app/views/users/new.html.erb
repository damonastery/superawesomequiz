<div class="session-container">
  <div class="well well-none">
    <h1>Sign Up</h1>
    <div class="form-group">
      <%= form_for(@user) do |f| %>
      <%= render 'fields', f: f %>
      <br>
      <%= f.submit "Create My Account", class: "form-control btn btn-info" %>
      <% end %>
    </div>
  </div>
</div>